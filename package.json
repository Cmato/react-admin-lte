{
  "name": "reactjs-admin-lte",
  "version": "0.6.0",
  "description": "Admin LTE components build with React",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-admin-lte/reactjs-admin-lte.git"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:umd:dev && yarn build:umd:prod && yarn build:copy-files",
    "build:es2015": "cross-env NODE_ENV=production babel src/ --ignore *.test.js -d build/",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel src/index.js -o build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src/ --ignore *.test.js -d build/es",
    "build:umd:dev": "webpack --config scripts/umd.webpack.config.js",
    "build:umd:prod": "cross-env NODE_ENV=production webpack --config scripts/umd.webpack.config.js",
    "build:copy-files": "babel-node scripts/copy-files.js",
    "test": "yarn typescript && yarn test:unit",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test:unit --watch",
    "test:coverage": "yarn test:unit --coverage",
    "typescript": "tsc -p tsconfig.json"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "peerDependencies": {
    "@react-bootstrap/pagination": "^1.0.0",
    "react": ">=0.14.0",
    "react-bootstrap": ">=0.30.0",
    "react-dom": ">=0.14.0",
    "react-fontawesome": ">=1.1.0"
  },
  "dependencies": {
    "airbnb-prop-types": "^2.8.1",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "uncontrollable": "^5.0.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@react-bootstrap/pagination": "^1.0.0",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.3",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.38",
    "@types/react-bootstrap": "^0.32.4",
    "@types/react-fontawesome": "^1.6.2",
    "@types/react-test-renderer": "^16.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "^22.4.2",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.0",
    "typescript": "^2.7.2",
    "webpack": ">=3 <4"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test))\\.(js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/scripts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
